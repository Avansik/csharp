namespace ConsoleApp5
{
    partial class Freezer
    {
        private string brand;
        private int capacity;
        private double temperature;
        private bool isFrostFree;
        private string energyRating;
        public static int totalFreezers = 0;
        public static string commonEnergyRating = "A++";
        public Freezer(string brand, int capacity, double temperature, bool isFrostFree, string energyRating)
        {
            this.brand = brand;
            this.capacity = capacity;
            this.temperature = temperature;
            this.isFrostFree = isFrostFree;
            this.energyRating = energyRating;
            totalFreezers++;
        }
        public Freezer(string brand, int capacity): this(brand, capacity, -18.0, false, commonEnergyRating) { }
        public Freezer(string brand): this(brand, 200, -18.0, false, commonEnergyRating) { }
        public Freezer(): this("Unknown", 200, -18.0, false, commonEnergyRating) { }
        static Freezer()
        {
            totalFreezers = 0;
            commonEnergyRating = "A++";
        }
        public string Brand { get { return brand; } set { brand = value; } }
        public int Capacity { get { return capacity; } set { capacity = value; } }
        public double Temperature { get { return temperature; } set { temperature = value; } }
        public bool IsFrostFree { get { return isFrostFree; } set { isFrostFree = value; } }
        public string EnergyRating { get { return energyRating; } set { energyRating = value; } }
        public static int TotalFreezers { get { return totalFreezers; } }
        public static string CommonEnergyRating
        {
            get { return commonEnergyRating; }
            set { commonEnergyRating = value; }
        }
        public override string ToString()
        {
            return $"Brand: {brand}, Capacity: {capacity}L, Temperature: {temperature}°C, Frost Free: {isFrostFree}, Energy Rating: {energyRating}";
        }
    }
    internal class Program
    {
        static void Main(string[] args)
        {
            Freezer[] freezers = new Freezer[]
            {
                new Freezer("Samsung", 300, -20.0, true, "A++"),
                new Freezer("LG", 250),
                new Freezer("Bosch"),
                new Freezer(),
                new Freezer("Whirlpool", 220, -18.5, false, "A+")
            };
            foreach (var freezer in freezers)
            {
                Console.WriteLine(freezer);
            }
        }
    }
}















namespace ConsoleApp5
{
    partial class Freezer
    {
        private string brand;
        private int capacity;
        private double temperature;
        private bool isFrostFree;
        private string energyRating;
        public static int totalFreezers = 0;
        public static string commonEnergyRating = "A++";
        public Freezer(string brand, int capacity, double temperature, bool isFrostFree, string energyRating)
        {
            this.brand = brand;
            this.capacity = capacity;
            this.temperature = temperature;
            this.isFrostFree = isFrostFree;
            this.energyRating = energyRating;
            totalFreezers++;
        }
        public Freezer(string brand, int capacity): this(brand, capacity, -18.0, false, commonEnergyRating) { }
        public Freezer(string brand): this(brand, 200, -18.0, false, commonEnergyRating) { }
        public Freezer(): this("Unknown", 200, -18.0, false, commonEnergyRating) { }
        static Freezer()
        {
            totalFreezers = 0;
            commonEnergyRating = "A++";
        }
        public string Brand { get { return brand; } set { brand = value; } }
        public int Capacity { get { return capacity; } set { capacity = value; } }
        public double Temperature { get { return temperature; } set { temperature = value; } }
        public bool IsFrostFree { get { return isFrostFree; } set { isFrostFree = value; } }
        public string EnergyRating { get { return energyRating; } set { energyRating = value; } }
        public static int TotalFreezers { get { return totalFreezers; } }
        public static string CommonEnergyRating
        {
            get { return commonEnergyRating; }
            set { commonEnergyRating = value; }
        }
        public override string ToString()
        {
            return $"Brand: {brand}, Capacity: {capacity}L, Temperature: {temperature}°C, Frost Free: {isFrostFree}, Energy Rating: {energyRating}";
        }
    }
    internal class Program
    {
        static void Main(string[] args)
        {
            Freezer[] freezers = new Freezer[]
            {
                new Freezer("Samsung", 300, -20.0, true, "A++"),
                new Freezer("LG", 250),
                new Freezer("Bosch"),
                new Freezer(),
                new Freezer("Whirlpool", 220, -18.5, false, "A+")
            };
            foreach (var freezer in freezers)
            {
                Console.WriteLine(freezer);
            }
        }
    }
}








